{
  "version": 1,
  "applications": {
    "soodcity": {
      "type": "node",
      "root": "backend",
      "build": {
        "command": "npm install"
      },
      "start": {
        "command": "node server.js"
      }
    },
    "soodcityb": {
      "type": "static",
      "root": "frontend",
      "build": {
        "command": "npm run build",
        "directory": "build"
      }
    }
  }
}